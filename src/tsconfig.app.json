{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "outDir": "../out-tsc/app",
    "baseUrl": "./",
    "module": "es2015",
    "types": []
  },
  "exclude": [
    "test.ts",
    "**/*.spec.ts"
  ],
  "path": {
    "@service/*": [
      "app/service/*"
    ],
    "@pipe/*": [
      "app/pipe/*"
    ],
    "@app/*": [
      "app/*"
    ]
  }
}